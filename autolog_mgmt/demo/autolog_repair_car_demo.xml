<odoo>

    <record id='autolog_repair_car_bmw' model='autolog.repair.car'>
        <field name='reference'>REP00001</field>
        <field name='reference_id' ref="autolog_receive_car_bmw"/>
        <field name='repair_start_date'>2024-11-27</field>
        <field name='repair_end_date'>2024-11-29</field>
        <field name='repair_notes'>The bumper has been successfully repaired
            and restored to its original condition. The left door has been
            fully repaired, eliminating any dents or scratches.
        </field>
        <field name='priority'>1</field>
        <field name="services_ids" eval="[Command.link(ref('autolog_mgmt.autolog_service_bumper')),
         Command.link(ref('autolog_mgmt.autolog_service_left_door'))]"/>
    </record>


    <record id='autolog_repair_car_audi' model='autolog.repair.car'>
        <field name='reference'>REP00003</field>
        <field name='reference_id' ref="autolog_receive_car_audi"/>
        <field name='repair_start_date'>2024-11-16</field>
        <field name='repair_end_date'>2024-11-17</field>
        <field name='repair_notes'>The bumper has been fully restored, and the front hood is now free of any dents or scratches, returning the vehicle to its original appearance.
        </field>
        <field name='priority'>2</field>
        <field name="services_ids" eval="[Command.link(ref('autolog_mgmt.autolog_service_bumper')),
         Command.link(ref('autolog_mgmt.autolog_service_front_hood'))]"/>
    </record>

</odoo>

